package novo;
public class MediaAritmetica implements CalcularMedia {

    @Override
    public double calc(double... notas) {
        if (notas == null || notas.length == 0) {
            return 0.0;
        }

        double soma = 0;
        for (double nota : notas) {
            soma += nota;
        }

        // MÃ©dia = Soma das notas / Quantidade de notas
        return soma / notas.length;
    }
}
